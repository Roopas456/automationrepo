<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>standard</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>Genpact CCA</documentTitle>

		<!-- report name - displayed at top-nav -->
		<!-- <reportName>CCA QA Automation Report</reportName> -->


		<!-- report name - displayed at top-nav -->

		<!-- report headline - displayed at top-nav, after reportHeadline -->
		<reportName>CCA: AUTOMATION REPORT
        <![CDATA[
                <img src='.\img\GP_HTL.png' />
                   ]]></reportName>

		<!-- location of charts in the test view -->
		<!-- top, bottom -->
		<testViewChartLocation>top</testViewChartLocation>

		<!-- custom javascript -->
		<scripts>
            <![CDATA[
                $(document).ready(function() {
                
                
                  
                $('.brand-logo.blue.darken-3').css('display','none');
});
            ]]>
            <![CDATA[
                          $(document).ready(function() {
                
                
                  
                $('ul li:nth-child(2) a span').css('display','none');
});
            ]]>





		</scripts>

		<!-- custom styles -->
		<styles>
            <![CDATA[
            .report-name { text-align: center;padding-left:372px;font-size:20px;} .report-name > img { float: left;height: 90%;margin-center: 10px;margin-top: 2px;width: auto; }
              
            ]]>
		</styles>
	</configuration>
</extentreports>